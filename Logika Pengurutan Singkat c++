#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
	int c;
	cout << "input data : ";
	cin >> c;
	int arr[c];
	int n = sizeof(arr) / sizeof(arr[0]);
	for (int i = 0; i < c; i++)
	{
		cout << "data ke- : " << i << " : ";
		cin >> arr[i];
	}
	sort (arr , arr+n);
	cout << "data setelah di urutkan : ";
	for (int i = 0;i < n;i++)
	{
		cout << arr[i] << " ";
	}
	return 0;
}
